{
  "name": "@styled-link/eslint-config-typescript",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "release": "pnpm run build && pnpm publish && changelogen --release && git push --follow-tags"
