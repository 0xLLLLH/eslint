    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "changelogen": "^0.5.5",
